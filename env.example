# Przykładowy plik zmiennych środowiskowych dla Railway
# Skopiuj ten plik jako .env i wypełnij własnymi wartościami

# Database
DATABASE_URL=postgresql://user:password@host:5432/database_name

# API Keys
API_KEY=twoj_sekretny_klucz_api
SECRET_KEY=twoj_sekretny_klucz_aplikacji

# Application
NODE_ENV=production
PORT=3000
HOST=0.0.0.0

# Redis (jeśli używasz)
REDIS_URL=redis://default:password@host:6379

# Inne ustawienia
LOG_LEVEL=info
MAX_CONNECTIONS=100

